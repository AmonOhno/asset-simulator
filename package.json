{
  "name": "asset-simulator",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.9.0",
  "workspaces": [
    "web",
    "server",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev:server & npm run dev:web",
    "dev:web": "npx turbo run dev --filter=web",
    "dev:server": "npx turbo run dev --filter=server",
    "build": "npx turbo run build",
    "start": "npm run start:server & npm run start:web",
    "start:web": "npm run start --workspace=web",
    "start:server": "npm run start --workspace=server",
    "test": "npx turbo run test",
    "lint": "npx turbo run lint",
    "clean": "npx turbo run clean"
  },
  "dependencies": {
    "turbo": "^2.5.6",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^16.18.126",
    "turbo": "^2.5.6",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
  ,
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
