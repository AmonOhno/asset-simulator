{
  "name": "asset-simulator",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.9.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npx turbo run dev --filter=server",
    "dev:web": "npx turbo run dev --filter=web",
    "dev:server": "npx turbo run dev --filter=server",
    "dev:mobile": "npx turbo run dev --filter=mobile",
    "build": "npx turbo run build",
    "build:prod": "npx turbo run build --filter=@asset-simulator/shared --filter=web --filter=server",
    "start": "npx turbo run start",
    "start:web": "npm run start --workspace=web",
    "start:server": "npm run start --workspace=server",
    "start:mobile": "npm run start --workspace=mobile",
    "test": "npx turbo run test",
    "lint": "npx turbo run lint",
    "clean": "npx turbo run clean"
  },
  "dependencies": {
    "turbo": "^2.5.6"
  },
  "devDependencies": {
    "@types/node": "^16.18.126",
    "turbo": "^2.5.6",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
